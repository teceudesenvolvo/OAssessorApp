{
  "expo": {
    "name": "O Assessor",
    "slug": "oassessor-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101422"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lavoroservicos.oassessor",
      "buildNumber": "5",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#101422"
      },
      "package": "com.blutecnologias.oassessor",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "O Assessor utiliza sua localização para exibir tarefas e eleitores próximos no mapa e para validar sua presença durante as visitas."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "946ff14e-fdd0-447c-97d7-69ae4dd7b307"
      }
    }
  }
}
